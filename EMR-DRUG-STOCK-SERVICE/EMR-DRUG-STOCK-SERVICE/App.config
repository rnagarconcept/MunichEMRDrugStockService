<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="oracle.manageddataaccess.client" type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess, Version=4.122.23.1, Culture=neutral, PublicKeyToken=89b483f429c47342" />
	  <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net, Version=1.2.10.0, Culture=neutral, PublicKeyToken=1b44e1d426115821" />
  </configSections>
	<connectionStrings>
		<add name="DestinationConnectionString" connectionString="Data Source=158.101.226.175:1521/APEXPDB.SUBNET.VCN.ORACLEVCN.COM;User Id=HOSPITAL;Password=HOSPITAL;Min Pool Size=10;Connection Lifetime=100000;Connection Timeout=60;Incr Pool Size=5; Decr Pool Size=2;"/>
		<!--<add name="SourceConnectionString" connectionString="Server=EGCM-MUNP-PH01;Database=ClientDB;User Id=clinic;Password=Clinic@123!@#;Pooling=true;Min Pool Size=5;Max Pool Size=100;Connect Timeout=30;"/>-->
		<add name="SourceConnectionString" connectionString="Server=localhost;Database=PharmacyClientDB;Integrated Security=True;"/>
	</connectionStrings>
	<log4net>
		<appender name="FileAppender" type="log4net.Appender.RollingFileAppender">
			<PreserveLogFileNameExtension value="true" />
			<file value="logs\\log.log" />
			<datePattern value="dd.MM.yyyy'.munich-drug-sync.log'" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<maxSizeRollBackups value="20" />
			<maximumFileSize value="50MB" />
			<staticLogFileName value="false" />
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%-5level %date [%property{sessionId}, %property{pid}, %thread, %property{user}] %logger - %message%newline" />
			</layout>
			<filter type="log4net.Filter.LoggerMatchFilter">
				<param name="AcceptOnMatch" value="false" />
				<param name="LoggerToMatch" value="WebLogger." />
			</filter>
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="INFO" />
				<levelMax value="FATAL" />
			</filter>
		</appender>
		<root>
			<level value="DEBUG" />
			<appender-ref ref="FileAppender" />
		</root>
	</log4net>
	<appSettings>		
		<add key="SourceQuery" value="SELECT DrugCode, AvQty FROM [dbo].[vPharmacyStock] WITH(NOLOCK)" />
		<add key="SourceConnectionTimeout" value="30" />
		<add key="TransactionInterval" value="5" />
		<add key="EnabledDebugg" value="true" />
		<add key="PackageName" value="PHARMACY_DRUG_SYNC_INTEGRATION" />
	</appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <system.data>
    <DbProviderFactories>
      <remove invariant="Oracle.ManagedDataAccess.Client" />
      <add name="ODP.NET, Managed Driver" invariant="Oracle.ManagedDataAccess.Client" description="Oracle Data Provider for .NET, Managed Driver" type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess, Version=4.122.23.1, Culture=neutral, PublicKeyToken=89b483f429c47342" />
    </DbProviderFactories>
  </system.data>	
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Memory" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
